<Window x:Class="LiteSql.Preference"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LiteSql"
        mc:Ignorable="d"
        Style="{DynamicResource CustomWindowStyle}"
        Title="Preference" Height="262"  Width="821" ResizeMode="CanMinimize" WindowStyle="ToolWindow">
    <Grid>
        <Label x:Name="decryptLabel" Content="Decrypt Key" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="decryptKeyText" Text="iAr4dTqA30LMZpSH1hlOspGcnAn77DUkmog1U9l5siA=+B^erH^Tsr@7$6szk+" HorizontalAlignment="Left" Margin="161,14,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="652" Height="22"/>
        <Button x:Name="prefSaveButton" Content="Save" Margin="753,160,6,0" Click="prefSaveButton_Click" Height="22" VerticalAlignment="Top"/>
        <Label x:Name="cipherCompatibilityLabel" Content="Cipher Compatibility" HorizontalAlignment="Left" Margin="10,36,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="cipherCompatibilityText" Text="3" HorizontalAlignment="Left" Margin="161,41,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="652" Height="22"/>
