        <TextBox x:Name="cipherDefaultHmacAlgorithmText" Text="HMAC_SHA1" HorizontalAlignment="Left" Margin="161,118,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="652" Height="22"/>
        <TextBox x:Name="cipherDefaultKdfAlgorithmText" Text="PBKDF2_HMAC_SHA1" HorizontalAlignment="Left" Margin="161,144,0,0" TextWrapping="Wrap" Width="652" Height="22" VerticalAlignment="Top"/>

